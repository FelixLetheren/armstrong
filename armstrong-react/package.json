{
  "name": "armstrong-react",
  "version": "3.0.11",
  "description": "Rocketmakers Armstrong library of React components",
  "main": "./dist/index.js",
  "style": "./dist/style.scss",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build-ts": "tsc --pretty -p source -d",
    "build-scss": "node sass_concat.js",
    "build": "yarn run build-ts && yarn run build-scss",
    "watch-scss": "node sass_copy.js",
    "watch-ts": "tsc -p source -d -w",
    "watch": "parallelshell -v \"yarn run watch-ts\" \"yarn run watch-scss\"",
    "lint": "tslint -c tslint.json 'source/**/*.ts{,x}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-react.git"
  },
  "keywords": [
    "typescript",
    "React",
    "sass"
  ],
  "author": "developers@rocketmakers.com",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "tslib": "^1.9.3",
    "underscore": "^1.9.1"
  },
  "peerDependencies": {
    "@types/react": "16.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@types/node": "9.6.31",
    "@types/react": "16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/underscore": "^1.8.9",
    "chokidar": "^2.0.4",
    "import-resolve": "^0.4.0",
    "parallelshell": "^3.0.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.0.3"
  }
}