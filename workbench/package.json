{
  "name": "armstrong-workbench",
  "version": "1.5.0",
  "description": "Just a quick react / typescript ready dev server for front-end dev.",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm-link-local ../armstrong-react --dev",
    "serve": "browser-sync start --config ./config/browsersync.config.js",
    "start": "parallelshell -v \"webpack --config config/webpack.config.dev.js --watch\" \"npm run serve\"",
    "build-dev": "webpack --config config/webpack.config.dev.js"
  },
  "author": "Rocketmakers",
  "license": "MIT",
  "devDependencies": {
    "npm-link-local": "^1.1.0",
    "autoprefixer": "^6.7.7",
    "autoprefixer-loader": "^3.2.0",
    "browser-sync": "^2.18.8",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.2",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "typescript": "~2.2.2",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "@types/classnames": "^0.0.32",
    "@types/history": "^4.5.0",
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.4",
    "@types/underscore": "^1.8.0",
    "classnames": "^2.2.5",
    "moment": "^2.18.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1"
  }
}
