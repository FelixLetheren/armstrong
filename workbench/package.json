{
  "name": "ProjectSkeleton",
  "version": "1.5.0",
  "description": "Just a quick react / typescript ready dev server for front-end dev.",
  "main": "server.js",
  "scripts": {
    "postinstall": "typings install",
    "serve": "browser-sync start --server ./www --files ./www --config ./config/browsersync.config.js",
    "start": "parallelshell -v \"webpack --config config/webpack.config.dev.js --watch\" \"npm run serve\"",
    "build-dev": "webpack --config config/webpack.config.dev.js",
    "build": "webpack --config config/webpack.config.prod.js"
  },
  "author": "Rocketmakers",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.11.1",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "underscore": "1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react-router": "2.1.1",
     "react": "15.0.1",
    "react-dom": "15.0.1",
    "moment": "^2.13.0"
  }
}
