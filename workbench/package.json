{
  "name": "armstrong-workbench",
  "version": "1.5.0",
  "description": "Just a quick react / typescript ready dev server for front-end dev.",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm-link-local ../armstrong-react --dev",
    "serve": "browser-sync start --config ./config/browsersync.config.js",
    "start": "parallelshell -v \"webpack --config config/webpack.config.dev.js --watch\" \"npm run serve\"",
    "build-dev": "webpack --config config/webpack.config.dev.js"
  },
  "author": "Rocketmakers",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/history": "^4.6.0",
    "@types/node": "^8.0.24",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router": "^4.0.14",
    "@types/underscore": "^1.8.1",
    "autoprefixer": "^7.1.2",
    "autoprefixer-loader": "^3.2.0",
    "browser-sync": "^2.18.13",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "npm-link-local": "^1.1.0",
    "parallelshell": "^3.0.1",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.2",
    "typescript": "~2.4.2",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0"
  }
}
